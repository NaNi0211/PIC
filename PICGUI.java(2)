private int rightRow(int pc) {
    for (int i = 0; i < table_1.getRowCount(); i++) {
        String pcValue = (String) table_1.getValueAt(i, 1).toString().substring(0, 4);
        if (pcValue.trim().length() > 0 && Integer.parseInt(pcValue, 16) == pc) {
            return i;
        }
    }
    return -1;
}
